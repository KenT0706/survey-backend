{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/hr-self-assessment", "dest": "/api/hr-self-assessment.js" },
    { "src": "/api/management-responses", "dest": "/api/management.js" },
    { "src": "/api/user-survey", "dest": "/api/user-survey.js" },
    { "src": "/api/auth", "dest": "/api/auth.js" },
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/auth", "dest": "/api/auth.js" },
    { "src": "/", "dest": "/api/index.js" }
  ]
}